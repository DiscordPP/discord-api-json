{
  "Role": {
    "parser-data": {
      "docs_url": "https://discord.com/developers/docs/topics/permissions#role-object-role-structure"
    },
    "id": {
      "type": "snowflake",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "role id"
      }
    },
    "name": {
      "type": "string",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "role name"
      }
    },
    "color": {
      "type": "integer",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "integer representation of hexadecimal color code"
      }
    },
    "hoist": {
      "type": "boolean",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "if this role is pinned in the user listing"
      }
    },
    "icon": {
      "type": "string",
      "optional": true,
      "nullable": true,
      "comments": {
        "Description": "role icon hash"
      }
    },
    "unicode_emoji": {
      "type": "string",
      "optional": true,
      "nullable": true,
      "comments": {
        "Description": "role unicode emoji"
      }
    },
    "position": {
      "type": "integer",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "position of this role"
      }
    },
    "permissions": {
      "type": "string",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "permission bit set"
      }
    },
    "managed": {
      "type": "boolean",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "whether this role is managed by an integration"
      }
    },
    "mentionable": {
      "type": "boolean",
      "optional": false,
      "nullable": false,
      "comments": {
        "Description": "whether this role is mentionable"
      }
    },
    "tags": {
      "type": "role tags object",
      "optional": true,
      "nullable": false,
      "comments": {
        "Description": "the tags this role has"
      }
    }
  },
  "RoleTags": {
    "parser-data": {
      "docs_url": "https://discord.com/developers/docs/topics/permissions#role-object-role-tags-structure"
    },
    "bot_id": {
      "type": "snowflake",
      "optional": true,
      "nullable": false,
      "comments": {
        "Description": "the id of the bot this role belongs to"
      }
    },
    "integration_id": {
      "type": "snowflake",
      "optional": true,
      "nullable": false,
      "comments": {
        "Description": "the id of the integration this role belongs to"
      }
    },
    "premium_subscriber": {
      "type": "null",
      "optional": true,
      "nullable": false,
      "comments": {
        "Description": "whether this is the guild's premium subscriber role"
      }
    }
  }
}