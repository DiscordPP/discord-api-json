{
  "Role": {
    "id": {
      "type": "snowflake",
      "description": "role id",
      "optional": false,
      "nullable": false
    },
    "name": {
      "type": "string",
      "description": "role name",
      "optional": false,
      "nullable": false
    },
    "color": {
      "type": "integer",
      "description": "integer representation of hexadecimal color code",
      "optional": false,
      "nullable": false
    },
    "hoist": {
      "type": "boolean",
      "description": "if this role is pinned in the user listing",
      "optional": false,
      "nullable": false
    },
    "icon": {
      "type": "string",
      "description": "role icon hash",
      "optional": true,
      "nullable": true
    },
    "unicode_emoji": {
      "type": "string",
      "description": "role unicode emoji",
      "optional": true,
      "nullable": true
    },
    "position": {
      "type": "integer",
      "description": "position of this role",
      "optional": false,
      "nullable": false
    },
    "permissions": {
      "type": "string",
      "description": "permission bit set",
      "optional": false,
      "nullable": false
    },
    "managed": {
      "type": "boolean",
      "description": "whether this role is managed by an integration",
      "optional": false,
      "nullable": false
    },
    "mentionable": {
      "type": "boolean",
      "description": "whether this role is mentionable",
      "optional": false,
      "nullable": false
    },
    "tags": {
      "type": "role tags object",
      "description": "the tags this role has",
      "optional": true,
      "nullable": false
    }
  },
  "Role Tags": {
    "bot_id": {
      "type": "snowflake",
      "description": "the id of the bot this role belongs to",
      "optional": true,
      "nullable": false
    },
    "integration_id": {
      "type": "snowflake",
      "description": "the id of the integration this role belongs to",
      "optional": true,
      "nullable": false
    },
    "premium_subscriber": {
      "type": "null",
      "description": "whether this is the guild's premium subscriber role",
      "optional": true,
      "nullable": false
    }
  }
}