    "build drm-wrap": {
      "level": 2,
      "url": "https://discord.com/developers/docs/dispatch/list-of-commands#build-drm-wrap",
      "content": [
        "Wraps your executable in Discord's DRM. This only works for Windows executables. If you want to DRM wrap a unix executable, you'll need to instead use ValidateOrExit.",
        "> danger\n> This action is destructive and overwrites the executable. Make sure you've got a backup handy if needed!",
        "|Arguments",
        "|Example"
      ],
      "Arguments": {
        "level": 6,
        "url": "https://discord.com/developers/docs/dispatch/list-of-commands#build-drm-wrap-arguments",
        "content": [
          [
            [
              "name",
              "values",
              "description"
            ],
            [
              "application_id",
              "int",
              "your application ID/client ID"
            ],
            [
              "path_to_executable",
              "file path",
              "the path to the executable, either explicit or relative to the dispatch executable"
            ]
          ]
        ]
      },
      "Example": {
        "level": 6,
        "url": "https://discord.com/developers/docs/dispatch/list-of-commands#build-drm-wrap-example",
        "content": [
          {
            "language": "",
            "code": "-> dispatch build drm-wrap 290926444748734465 C:\\my-game\\my-game.exe\n"
          }
        ]
      }
    },
    "build list": {
      "level": 2,
      "url": "https://discord.com/developers/docs/dispatch/list-of-commands#build-list",
      "content": [
        "Lists the builds available on the given branch.",
        "|Arguments",
        "|Example"
      ],
      "Arguments": {
        "level": 6,
        "url": "https://discord.com/developers/docs/dispatch/list-of-commands#build-list-arguments",
        "content": [
          [
            [
              "name",
              "values",
              "description"
            ],
            [
              "application_id",
              "int",
              "your application ID/client ID"
            ],
            [
              "branch_id",
              "int",
              "the id of the branch to check"
            ]
          ]
        ]
      },
      "Example": {
        "level": 6,
        "url": "https://discord.com/developers/docs/dispatch/list-of-commands#build-list-example",
        "content": [
          {
            "language": "",
            "code": "-> dispatch build list 290926444748734465 491362538965958686\n|    APPLICATION ID    |       BUILD ID       |        STATUS        |   CREATION BRANCH    |           CREATED AT           |\n| -------------------- | -------------------- | -------------------- | -------------------- | ------------------------------ |\n|  290926444748734465  |  489230031839821824  |        READY         |        master        |  2018-09-12 00:25:29.045554Z   |\n|  290926444748734465  |  479469321974841354  |        READY         |        master        |  2018-08-16 01:59:54.481336Z   |\n"
          }
        ]
      }
