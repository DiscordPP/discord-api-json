{
  "List Guild Emojis": {
    "command": "GET",
    "url": "/guilds/{guild.id}/emojis"
  },
  "Get Guild Emoji": {
    "command": "GET",
    "url": "/guilds/{guild.id}/emojis/{emoji.id}"
  },
  "Create Guild Emoji": {
    "command": "POST",
    "url": "/guilds/{guild.id}/emojis",
    "json": {
      "parser-data": {
        "docs_url": "https://discord.com/developers/docs/resources/emoji#emoji-object"
      },
      "name": {
        "type": "string",
        "description": "name of the emoji",
        "optional": false,
        "nullable": false
      },
      "image": {
        "type": "image data",
        "description": "the 128x128 emoji image",
        "optional": false,
        "nullable": false
      },
      "roles": {
        "type": "array of snowflakes",
        "description": "roles allowed to use this emoji",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Modify Guild Emoji": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/emojis/{emoji.id}",
    "json": {
      "parser-data": {
        "docs_url": "https://discord.com/developers/docs/resources/emoji#emoji-object"
      },
      "name": {
        "type": "string",
        "description": "name of the emoji",
        "optional": false,
        "nullable": false
      },
      "roles": {
        "type": "array of snowflakes",
        "description": "roles allowed to use this emoji",
        "optional": false,
        "nullable": true
      }
    }
  },
  "Delete Guild Emoji": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/emojis/{emoji.id}"
  }
}