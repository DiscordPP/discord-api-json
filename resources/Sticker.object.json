      "optional": false,
      "nullable": false
    },
    "name": {
      "type": "string",
      "description": "name of the sticker",
      "optional": false,
      "nullable": false
    },
    "format_type": {
      "type": "integer",
      "description": "type of sticker format",
      "optional": false,
      "nullable": false
    }
