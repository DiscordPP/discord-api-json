    "id": {
      "type": "snowflake",
      "description": "id of the sticker",
      "optional": false,
      "nullable": false
    },
    "pack_id": {
      "type": "snowflake",
      "description": "for standard stickers, id of the pack the sticker is from",
      "optional": true,
      "nullable": false
    },
    "name": {
      "type": "string",
      "description": "name of the sticker",
      "optional": false,
      "nullable": false
