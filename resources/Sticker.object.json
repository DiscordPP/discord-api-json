      "description": "description of the sticker",
      "optional": false,
      "nullable": true
    },
    "tags": {
      "type": "string",
      "description": "autocomplete/suggestion tags for the sticker (max 200 characters)",
      "optional": false,
      "nullable": false,
      "note-field": "A comma separated list of keywords is the format used in this field by standard stickers, but this is just a convention.Incidentally the client will always use a name generated from an emoji as the value of this field when creating or modifying a guild sticker."
    },
    "asset": {
      "type": "string",
      "description": "Deprecated previously the sticker asset hash, now an empty string",
      "optional": true,
      "nullable": false
    },
    "type": {
