{
  "Get Current User": {
    "command": "GET",
    "url": "/users/@me"
  },
  "Get User": {
    "command": "GET",
    "url": "/users/{user.id}"
  },
  "Modify Current User": {
    "command": "PATCH",
    "url": "/users/@me",
    "json": {
      "username": {
        "type": "string",
        "description": "user's username, if changed may cause the user's discriminator to be randomized.",
        "optional": false,
        "nullable": false
      },
      "avatar": {
        "type": "image data",
        "description": "if passed, modifies the user's avatar",
        "optional": false,
        "nullable": true
      }
    }
  },
  "Get Current User Guilds": {
    "command": "GET",
    "url": "/users/@me/guilds",
    "query": {
      "before": {
        "type": "snowflake",
        "description": "get guilds before this guild ID",
        "required": "false",
        "default": "absent",
        "optional": false,
        "nullable": false
      },
      "after": {
        "type": "snowflake",
        "description": "get guilds after this guild ID",
        "required": "false",
        "default": "absent",
        "optional": false,
        "nullable": false
      },
      "limit": {
        "type": "integer",
        "description": "max number of guilds to return (1-200)",
        "required": "false",
        "default": "200",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Get Current User Guild Member": {
    "command": "GET",
    "url": "/users/@me/guilds/{guild.id}/member"
  },
  "Leave Guild": {
    "command": "DELETE",
    "url": "/users/@me/guilds/{guild.id}"
  },
  "Create DM": {
    "command": "POST",
    "url": "/users/@me/channels",
    "json": {
      "recipient_id": {
        "type": "snowflake",
        "description": "the recipient to open a DM channel with",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Create Group DM": {
    "command": "POST",
    "url": "/users/@me/channels",
    "json": {
      "access_tokens": {
        "type": "array of strings",
        "description": "access tokens of users that have granted your app the gdm.join scope",
        "optional": false,
        "nullable": false
      },
      "nicks": {
        "type": "dict",
        "description": "a dictionary of user ids to their respective nicknames",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Get User Connections": {
    "command": "GET",
    "url": "/users/@me/connections"
  }
}