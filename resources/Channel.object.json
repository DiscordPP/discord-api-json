      "nullable": false
    }
  },
  "Channel Mention": {
    "id": {
      "type": "snowflake",
      "description": "id of the channel",
      "optional": false,
      "nullable": false
    },
    "guild_id": {
      "type": "snowflake",
      "description": "id of the guild containing the channel",
      "optional": false,
      "nullable": false
    },
    "type": {
      "type": "integer",
      "description": "the type of channel",
