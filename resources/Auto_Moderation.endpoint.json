{
  "List Auto Moderation Rules for Guild": {
    "command": "GET",
    "url": "/guilds/{guild.id}/auto-moderation/rules"
  },
  "Get Auto Moderation Rule": {
    "command": "GET",
    "url": "/guilds/{guild.id}/auto-moderation/rules/{auto_moderation_rule.id}"
  },
  "Create Auto Moderation Rule": {
    "command": "POST",
    "url": "/guilds/{guild.id}/auto-moderation/rules",
    "json": {
      "name": {
        "type": "string",
        "description": "the rule name",
        "optional": false,
        "nullable": false
      },
      "event_type": {
        "type": "integer",
        "description": "the event type",
        "optional": false,
        "nullable": false
      },
      "trigger_type": {
        "type": "integer",
        "description": "the trigger type",
        "optional": false,
        "nullable": false
      },
      "trigger_metadata": {
        "type": "object",
        "description": "the trigger metadata",
        "note": "*",
        "optional": true,
        "nullable": false
      },
      "actions": {
        "type": "array of action objects",
        "description": "the actions which will execute when the rule is triggered",
        "optional": false,
        "nullable": false
      },
      "enabled": {
        "type": "boolean",
        "description": "whether the rule is enabled (False by default)",
        "optional": true,
        "nullable": false
      },
      "exempt_roles": {
        "type": "array of snowflakes",
        "description": "the role ids that should not be affected by the rule (Maximum of 20)",
        "optional": true,
        "nullable": false
      },
      "exempt_channels": {
        "type": "array of snowflakes",
        "description": "the channel ids that should not be affected by the rule (Maximum of 50)",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Modify Auto Moderation Rule": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/auto-moderation/rules/{auto_moderation_rule.id}",
    "json": {
      "name": {
        "type": "string",
        "description": "the rule name",
        "optional": false,
        "nullable": false
      },
      "event_type": {
        "type": "integer",
        "description": "the event type",
        "optional": false,
        "nullable": false
      },
      "trigger_metadata": {
        "type": "object",
        "description": "the trigger metadata",
        "note": "*",
        "optional": true,
        "nullable": false
      },
      "actions": {
        "type": "array of action objects",
        "description": "the actions which will execute when the rule is triggered",
        "optional": false,
        "nullable": false
      },
      "enabled": {
        "type": "boolean",
        "description": "whether the rule is enabled",
        "optional": false,
        "nullable": false
      },
      "exempt_roles": {
        "type": "array of snowflakes",
        "description": "the role ids that should not be affected by the rule (Maximum of 20)",
        "optional": false,
        "nullable": false
      },
      "exempt_channels": {
        "type": "array of snowflakes",
        "description": "the channel ids that should not be affected by the rule (Maximum of 50)",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Delete Auto Moderation Rule": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/auto-moderation/rules/{auto_moderation_rule.id}"
  }
}