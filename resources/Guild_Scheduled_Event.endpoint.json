{
  "List Scheduled Events for Guild": {
    "command": "GET",
    "url": "/guilds/{guild.id}/scheduled-events",
    "query": [
      {
        "field": "with_user_count",
        "type": "boolean",
        "description": "include number of users subscribed to each event",
        "optional": true,
        "nullable": false
      }
    ]
  },
  "Create Guild Scheduled Event": {
    "command": "POST",
    "url": "/guilds/{guild.id}/scheduled-events",
    "json": [
      {
        "field": "channel_id",
        "type": "snowflake *",
        "description": "the channel id of the scheduled event.",
        "note": "*",
        "optional": true,
        "nullable": false
      },
      {
        "field": "entity_metadata",
        "type": "entity metadata",
        "description": "the entity metadata of the scheduled event",
        "note": "**",
        "optional": true,
        "nullable": false
      },
      {
        "field": "name",
        "type": "string",
        "description": "the name of the scheduled event",
        "optional": false,
        "nullable": false
      },
      {
        "field": "privacy_level",
        "type": "privacy level",
        "description": "the privacy level of the scheduled event",
        "optional": false,
        "nullable": false
      },
      {
        "field": "scheduled_start_time",
        "type": "ISO8601 timestamp",
        "description": "the time to schedule the scheduled event",
        "optional": false,
        "nullable": false
      },
      {
        "field": "scheduled_end_time",
        "type": "ISO8601 timestamp",
        "description": "the time when the scheduled event is scheduled to end",
        "note": "**",
        "optional": true,
        "nullable": false
      },
      {
        "field": "description",
        "type": "string",
        "description": "the description of the scheduled event",
        "optional": true,
        "nullable": false
      },
      {
        "field": "entity_type",
        "type": "entity type",
        "description": "the entity type of the scheduled event",
        "optional": false,
        "nullable": false
      },
      {
        "field": "image",
        "type": "image data",
        "description": "the cover image of the scheduled event",
        "optional": true,
        "nullable": false
      }
    ]
  },
  "Get Guild Scheduled Event": {
    "command": "GET",
    "url": "/guilds/{guild.id}/scheduled-events/{guild_scheduled_event.id}",
    "query": [
      {
        "field": "with_user_count",
        "type": "boolean",
        "description": "include number of users subscribed to this event",
        "optional": true,
        "nullable": false
      }
    ]
  },
  "Modify Guild Scheduled Event": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/scheduled-events/{guild_scheduled_event.id}",
    "json": [
      {
        "field": "channel_id",
        "type": "snowflake",
        "description": "the channel id of the scheduled event, set to null if changing entity type to EXTERNAL",
        "note": "*",
        "optional": true,
        "nullable": true
      },
      {
        "field": "entity_metadata",
        "type": "entity metadata",
        "description": "the entity metadata of the scheduled event",
        "optional": true,
        "nullable": true
      },
      {
        "field": "name",
        "type": "string",
        "description": "the name of the scheduled event",
        "optional": true,
        "nullable": false
      },
      {
        "field": "privacy_level",
        "type": "privacy level",
        "description": "the privacy level of the scheduled event",
        "optional": true,
        "nullable": false
      },
      {
        "field": "scheduled_start_time",
        "type": "ISO8601 timestamp",
        "description": "the time to schedule the scheduled event",
        "optional": true,
        "nullable": false
      },
      {
        "field": "scheduled_end_time",
        "type": "ISO8601 timestamp",
        "description": "the time when the scheduled event is scheduled to end",
        "note": "*",
        "optional": true,
        "nullable": false
      },
      {
        "field": "description",
        "type": "string",
        "description": "the description of the scheduled event",
        "optional": true,
        "nullable": true
      },
      {
        "field": "entity_type",
        "type": "event entity type",
        "description": "the entity type of the scheduled event",
        "note": "*",
        "optional": true,
        "nullable": false
      },
      {
        "field": "status",
        "type": "event status",
        "description": "the status of the scheduled event",
        "optional": true,
        "nullable": false
      },
      {
        "field": "image",
        "type": "image data",
        "description": "the cover image of the scheduled event",
        "optional": true,
        "nullable": false
      }
    ]
  },
  "Delete Guild Scheduled Event": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/scheduled-events/{guild_scheduled_event.id}"
  },
  "Get Guild Scheduled Event Users": {
    "command": "GET",
    "url": "/guilds/{guild.id}/scheduled-events/{guild_scheduled_event.id}/users",
    "query": [
      {
        "field": "limit",
        "type": "number",
        "description": "number of users to return (up to maximum 100)",
        "default": "100",
        "optional": true,
        "nullable": false
      },
      {
        "field": "with_member",
        "type": "boolean",
        "description": "include guild member data if it exists",
        "default": "false",
        "optional": true,
        "nullable": false
      },
      {
        "field": "before",
        "type": "snowflake",
        "description": "consider only users before given user id",
        "default": "null",
        "note": "*",
        "optional": true,
        "nullable": false
      },
      {
        "field": "after",
        "type": "snowflake",
        "description": "consider only users after given user id",
        "default": "null",
        "note": "*",
        "optional": true,
        "nullable": false
      }
    ]
  }
}