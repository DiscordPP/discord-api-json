{
  "Get Guild Template": {
    "command": "GET",
    "url": "/guilds/templates/{template.code}"
  },
  "Create Guild from Guild Template": {
    "command": "POST",
    "url": "/guilds/templates/{template.code}",
    "json": {
      "name": {
        "type": "string",
        "description": "name of the guild (2-100 characters)",
        "optional": false,
        "nullable": false
      },
      "icon": {
        "type": "image data",
        "description": "base64 128x128 image for the guild icon",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Get Guild Templates": {
    "command": "GET",
    "url": "/guilds/{guild.id}/templates"
  },
  "Create Guild Template": {
    "command": "POST",
    "url": "/guilds/{guild.id}/templates",
    "json": {
      "name": {
        "type": "string",
        "description": "name of the template (1-100 characters)",
        "optional": false,
        "nullable": false
      },
      "description": {
        "type": "string",
        "description": "description for the template (0-120 characters)",
        "optional": true,
        "nullable": true
      }
    }
  },
  "Sync Guild Template": {
    "command": "PUT",
    "url": "/guilds/{guild.id}/templates/{template.code}"
  },
  "Modify Guild Template": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/templates/{template.code}",
    "json": {
      "name": {
        "type": "string",
        "description": "name of the template (1-100 characters)",
        "optional": true,
        "nullable": false
      },
      "description": {
        "type": "string",
        "description": "description for the template (0-120 characters)",
        "optional": true,
        "nullable": true
      }
    }
  },
  "Delete Guild Template": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/templates/{template.code}"
  }
}