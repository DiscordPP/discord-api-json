{
  "Get Guild Template": {
    "Command": "GET",
    "URL": "/guilds/templates/{template.code}"
  },
  "Create Guild from Guild Template": {
    "Command": "POST",
    "URL": "/guilds/templates/{template.code}",
    "json": [
      {
        "Field": "name",
        "Type": "string",
        "Description": "name of the guild (2-100 characters)",
        "Optional": false,
        "Nullable": false
      },
      {
        "Field": "icon",
        "Type": "image data",
        "Description": "base64 128x128 image for the guild icon",
        "Optional": true,
        "Nullable": false
      }
    ]
  },
  "Get Guild Templates": {
    "Command": "GET",
    "URL": "/guilds/{guild.id}/templates"
  },
  "Create Guild Template": {
    "Command": "POST",
    "URL": "/guilds/{guild.id}/templates",
    "json": [
      {
        "Field": "name",
        "Type": "string",
        "Description": "name of the template (1-100 characters)",
        "Optional": false,
        "Nullable": false
      },
      {
        "Field": "description",
        "Type": "string",
        "Description": "description for the template (0-120 characters)",
        "Optional": true,
        "Nullable": true
      }
    ]
  },
  "Sync Guild Template": {
    "Command": "PUT",
    "URL": "/guilds/{guild.id}/templates/{template.code}"
  },
  "Modify Guild Template": {
    "Command": "PATCH",
    "URL": "/guilds/{guild.id}/templates/{template.code}",
    "json": [
      {
        "Field": "name",
        "Type": "string",
        "Description": "name of the template (1-100 characters)",
        "Optional": true,
        "Nullable": false
      },
      {
        "Field": "description",
        "Type": "string",
        "Description": "description for the template (0-120 characters)",
        "Optional": true,
        "Nullable": true
      }
    ]
  },
  "Delete Guild Template": {
    "Command": "DELETE",
    "URL": "/guilds/{guild.id}/templates/{template.code}"
  }
}