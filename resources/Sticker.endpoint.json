{
  "Get Sticker": {
    "command": "GET",
    "url": "/stickers/{sticker.id}"
  },
  "List Nitro Sticker Packs": {
    "command": "GET",
    "url": "/sticker-packs"
  },
  "List Guild Stickers": {
    "command": "GET",
    "url": "/guilds/{guild.id}/stickers"
  },
  "Get Guild Sticker": {
    "command": "GET",
    "url": "/guilds/{guild.id}/stickers/{sticker.id}"
  },
  "Create Guild Sticker": {
    "command": "POST",
    "url": "/guilds/{guild.id}/stickers"
  },
  "Modify Guild Sticker": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/stickers/{sticker.id}",
    "json": [
      {
        "field": "name",
        "type": "string",
        "description": "name of the sticker (2-30 characters)",
        "optional": false,
        "nullable": false
      },
      {
        "field": "description",
        "type": "string",
        "description": "description of the sticker (2-100 characters)",
        "optional": false,
        "nullable": true
      },
      {
        "field": "tags",
        "type": "string",
        "description": "autocomplete/suggestion tags for the sticker (max 200 characters)",
        "optional": false,
        "nullable": false
      }
    ]
  },
  "Delete Guild Sticker": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/stickers/{sticker.id}"
  }
}