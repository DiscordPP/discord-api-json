{
  "Auto Moderation Rule": {
    "discord-api-json": {
      "docs_url": "https://discord.com/developers/docs/resources/auto-moderation#auto-moderation-rule-object"
    },
    "id": {
      "type": "snowflake",
      "description": "the id of this rule",
      "optional": false,
      "nullable": false
    },
    "guild_id": {
      "type": "snowflake",
      "description": "the id of the guild which this rule belongs to",
      "optional": false,
      "nullable": false
    },
    "name": {
      "type": "string",
      "description": "the rule name",
      "optional": false,
      "nullable": false
    },
    "creator_id": {
      "type": "snowflake",
      "description": "the user which first created this rule",
      "optional": false,
      "nullable": false
    },
    "event_type": {
      "type": "integer",
      "description": "the rule event type",
      "optional": false,
      "nullable": false
    },
    "trigger_type": {
      "type": "integer",
      "description": "the rule trigger type",
      "optional": false,
      "nullable": false
    },
    "trigger_metadata": {
      "type": "object",
      "description": "the rule trigger metadata",
      "optional": false,
      "nullable": false
    },
    "actions": {
      "type": "array of action objects",
      "description": "the actions which will execute when the rule is triggered",
      "optional": false,
      "nullable": false
    },
    "enabled": {
      "type": "boolean",
      "description": "whether the rule is enabled",
      "optional": false,
      "nullable": false
    },
    "exempt_roles": {
      "type": "array of snowflakes",
      "description": "the role ids that should not be affected by the rule (Maximum of 20)",
      "optional": false,
      "nullable": false
    },
    "exempt_channels": {
      "type": "array of snowflakes",
      "description": "the channel ids that should not be affected by the rule (Maximum of 50)",
      "optional": false,
      "nullable": false
    }
  },
  "Auto Moderation Action": {
    "discord-api-json": {
      "docs_url": "https://discord.com/developers/docs/resources/auto-moderation#auto-moderation-action-object"
    },
    "type": {
      "type": "action type",
      "description": "the type of action",
      "optional": false,
      "nullable": false
    },
    "metadata": {
      "type": "action metadata",
      "description": "additional metadata needed during execution for this specific action type",
      "optional": true,
      "nullable": false,
      "note-field": "Can be omitted based on type. See the Associated Action Types column in action metadata to understand which type values require metadata to be set."
    }
  }
}