{
  "Get Invite": {
    "command": "GET",
    "url": "/invites/{invite.code}",
    "query": {
      "discord-api-json": {
        "docs_url": "https://discord.com/developers/docs/resources/invite#invite-stage-instance-object"
      },
      "with_counts": {
        "type": "boolean",
        "description": "whether the invite should contain approximate member counts",
        "optional": true,
        "nullable": false
      },
      "with_expiration": {
        "type": "boolean",
        "description": "whether the invite should contain the expiration date",
        "optional": true,
        "nullable": false
      },
      "guild_scheduled_event_id": {
        "type": "snowflake",
        "description": "the guild scheduled event to include with the invite",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Delete Invite": {
    "command": "DELETE",
    "url": "/invites/{invite.code}"
  }
}