{
  "Create Guild": {
    "command": "POST",
    "url": "/guilds",
    "docs_url": "https://discord.com/developers/docs/resources/guild#create-guild",
    "json": {
      "name": {
        "type": "string",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "name of the guild (2-100 characters)"
        }
      },
      "region": {
        "type": "string",
        "optional": true,
        "nullable": true,
        "comments": {
          "Description": "voice region id (deprecated)"
        }
      },
      "icon": {
        "type": "image data",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "base64 128x128 image for the guild icon"
        }
      },
      "verification_level": {
        "type": "integer",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "verification level"
        }
      },
      "default_message_notifications": {
        "type": "integer",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "default message notification level"
        }
      },
      "explicit_content_filter": {
        "type": "integer",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "explicit content filter level"
        }
      },
      "roles": {
        "type": "array of role objects",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "new guild roles"
        }
      },
      "channels": {
        "type": "array of partial channel objects",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "new guild's channels"
        }
      },
      "afk_channel_id": {
        "type": "snowflake",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "id for afk channel"
        }
      },
      "afk_timeout": {
        "type": "integer",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "afk timeout in seconds, can be set to: 60, 300, 900, 1800, 3600"
        }
      },
      "system_channel_id": {
        "type": "snowflake",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "the id of the channel where guild notices such as welcome messages and boost events are posted"
        }
      },
      "system_channel_flags": {
        "type": "integer",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "system channel flags"
        }
      }
    }
  },
  "Get Guild": {
    "command": "GET",
    "url": "/guilds/{guild.id}",
    "docs_url": "https://discord.com/developers/docs/resources/guild#get-guild",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    },
    "query string": {
      "with_counts": {
        "type": "boolean",
        "optional": true,
        "nullable": false,
        "comments": {
          "Description": "when true, will return approximate member and presence counts for the guild",
          "Required": "false",
          "Default": "false"
        }
      }
    }
  },
  "Get Guild Preview": {
    "command": "GET",
    "url": "/guilds/{guild.id}/preview",
    "docs_url": "https://discord.com/developers/docs/resources/guild#get-guild-preview",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    }
  },
  "Modify Guild": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}",
    "docs_url": "https://discord.com/developers/docs/resources/guild#modify-guild",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    },
    "json": {
      "name": {
        "type": "string",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "guild name"
        }
      },
      "region": {
        "type": "string",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "guild voice region id (deprecated)"
        }
      },
      "verification_level": {
        "type": "integer",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "verification level"
        }
      },
      "default_message_notifications": {
        "type": "integer",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "default message notification level"
        }
      },
      "explicit_content_filter": {
        "type": "integer",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "explicit content filter level"
        }
      },
      "afk_channel_id": {
        "type": "snowflake",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "id for afk channel"
        }
      },
      "afk_timeout": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "afk timeout in seconds, can be set to: 60, 300, 900, 1800, 3600"
        }
      },
      "icon": {
        "type": "image data",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "base64 1024x1024 png/jpeg/gif image for the guild icon (can be animated gif when the server has the ANIMATED_ICON feature)"
        }
      },
      "owner_id": {
        "type": "snowflake",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "user id to transfer guild ownership to (must be owner)"
        }
      },
      "splash": {
        "type": "image data",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "base64 16:9 png/jpeg image for the guild splash (when the server has the INVITE_SPLASH feature)"
        }
      },
      "discovery_splash": {
        "type": "image data",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "base64 16:9 png/jpeg image for the guild discovery splash (when the server has the DISCOVERABLE feature)"
        }
      },
      "banner": {
        "type": "image data",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "base64 16:9 png/jpeg image for the guild banner (when the server has the BANNER feature; can be animated gif when the server has the ANIMATED_BANNER feature)"
        }
      },
      "system_channel_id": {
        "type": "snowflake",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "the id of the channel where guild notices such as welcome messages and boost events are posted"
        }
      },
      "system_channel_flags": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "system channel flags"
        }
      },
      "rules_channel_id": {
        "type": "snowflake",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "the id of the channel where Community guilds display rules and/or guidelines"
        }
      },
      "public_updates_channel_id": {
        "type": "snowflake",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "the id of the channel where admins and moderators of Community guilds receive notices from Discord"
        }
      },
      "preferred_locale": {
        "type": "string",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "the preferred locale of a Community guild used in server discovery and notices from Discord; defaults to \"en-US\""
        }
      },
      "features": {
        "type": "array of guild feature strings",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "enabled guild features"
        }
      },
      "description": {
        "type": "string",
        "optional": false,
        "nullable": true,
        "comments": {
          "Description": "the description for the guild"
        }
      },
      "premium_progress_bar_enabled": {
        "type": "boolean",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "whether the guild's boost progress bar should be enabled"
        }
      }
    }
  },
  "Delete Guild": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}",
    "docs_url": "https://discord.com/developers/docs/resources/guild#delete-guild",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    }
  },
  "Get Guild Channels": {
    "command": "GET",
    "url": "/guilds/{guild.id}/channels",
    "docs_url": "https://discord.com/developers/docs/resources/guild#get-guild-channels",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    }
  },
  "Create Guild Channel": {
    "command": "POST",
    "url": "/guilds/{guild.id}/channels",
    "docs_url": "https://discord.com/developers/docs/resources/guild#create-guild-channel",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    },
    "json": {
      "name": {
        "type": "string",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "channel name (1-100 characters)",
          "Channel Type": "All"
        }
      },
      "type": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the type of channel",
          "Channel Type": "All"
        }
      },
      "topic": {
        "type": "string",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "channel topic (0-1024 characters)",
          "Channel Type": "Text, Announcement, Forum"
        }
      },
      "bitrate": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the bitrate (in bits) of the voice or stage channel; min 8000",
          "Name Note": "For voice channels, normal servers can set bitrate up to 96000, servers with Boost level 1 can set up to 128000, servers with Boost level 2 can set up to 256000, and servers with Boost level 3 or the VIP_REGIONS guild feature can set up to 384000. For stage channels, bitrate can be set up to 64000.",
          "Channel Type": "Voice, Stage"
        }
      },
      "user_limit": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the user limit of the voice channel",
          "Channel Type": "Voice, Stage"
        }
      },
      "rate_limit_per_user": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "amount of seconds a user has to wait before sending another message (0-21600); bots, as well as users with the permission manage_messages or manage_channel, are unaffected",
          "Channel Type": "Text, Forum"
        }
      },
      "position": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "sorting position of the channel",
          "Channel Type": "All"
        }
      },
      "permission_overwrites": {
        "type": "array of partial overwrite objects",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the channel's permission overwrites",
          "Name Note": "In each overwrite object, the allow and deny keys can be omitted or set to null, which both default to \"0\".",
          "Channel Type": "All"
        }
      },
      "parent_id": {
        "type": "snowflake",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "id of the parent category for a channel",
          "Channel Type": "Text, Voice, Announcement, Stage, Forum"
        }
      },
      "nsfw": {
        "type": "boolean",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "whether the channel is nsfw",
          "Channel Type": "Text, Voice, Announcement, Stage, Forum"
        }
      },
      "rtc_region": {
        "type": "string",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "channel voice region id of the voice or stage channel, automatic when set to null",
          "Channel Type": "Voice, Stage"
        }
      },
      "video_quality_mode": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the camera video quality mode of the voice channel",
          "Channel Type": "Voice, Stage"
        }
      },
      "default_auto_archive_duration": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the default duration that the clients use (not the API) for newly created threads in the channel, in minutes, to automatically archive the thread after recent activity",
          "Channel Type": "Text, Announcement, Forum"
        }
      },
      "default_reaction_emoji": {
        "type": "default reaction object",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "emoji to show in the add reaction button on a thread in a GUILD_FORUM channel",
          "Channel Type": "Forum"
        }
      },
      "available_tags": {
        "type": "array of tag objects",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "set of tags that can be used in a GUILD_FORUM channel",
          "Channel Type": "Forum"
        }
      },
      "default_sort_order": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the default sort order type used to order posts in GUILD_FORUM channels",
          "Channel Type": "Forum"
        }
      }
    }
  },
  "Modify Guild Channel Positions": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/channels",
    "docs_url": "https://discord.com/developers/docs/resources/guild#modify-guild-channel-positions",
    "url params": {
