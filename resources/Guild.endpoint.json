        "type": "snowflake"
      },
      "user.id": {
        "type": "snowflake"
      }
    }
  },
  "List Guild Members": {
    "command": "GET",
    "url": "/guilds/{guild.id}/members",
    "docs_url": "https://discord.com/developers/docs/resources/guild#list-guild-members",
    "url params": {
      "guild.id": {
        "type": "snowflake"
      }
    },
    "query string": {
      "limit": {
        "type": "integer",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "max number of members to return (1-1000)",
          "Default": "1"
        }
      },
      "after": {
        "type": "snowflake",
        "optional": false,
        "nullable": false,
        "comments": {
          "Description": "the highest user id in the previous page",
          "Default": "0"
        }
      }
    }
  },
  "Search Guild Members": {
    "command": "GET",
    "url": "/guilds/{guild.id}/members/search",
