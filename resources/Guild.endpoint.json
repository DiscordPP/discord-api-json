{
  "Create Guild": {
    "command": "POST",
    "url": "/guilds",
    "json": {
      "name": {
        "type": "string",
        "description": "name of the guild (2-100 characters)",
        "optional": false,
        "nullable": false
      },
      "region": {
        "type": "string",
        "description": "voice region id (deprecated)",
        "optional": true,
        "nullable": true
      },
      "icon": {
        "type": "image data",
        "description": "base64 128x128 image for the guild icon",
        "optional": true,
        "nullable": false
      },
      "verification_level": {
        "type": "integer",
        "description": "verification level",
        "optional": true,
        "nullable": false
      },
      "default_message_notifications": {
        "type": "integer",
        "description": "default message notification level",
        "optional": true,
        "nullable": false
      },
      "explicit_content_filter": {
        "type": "integer",
        "description": "explicit content filter level",
        "optional": true,
        "nullable": false
      },
      "roles": {
        "type": "array of role objects",
        "description": "new guild roles",
        "optional": true,
        "nullable": false
      },
      "channels": {
        "type": "array of partial channel objects",
        "description": "new guild's channels",
        "optional": true,
        "nullable": false
      },
      "afk_channel_id": {
        "type": "snowflake",
        "description": "id for afk channel",
        "optional": true,
        "nullable": false
      },
      "afk_timeout": {
        "type": "integer",
        "description": "afk timeout in seconds",
        "optional": true,
        "nullable": false
      },
      "system_channel_id": {
        "type": "snowflake",
        "description": "the id of the channel where guild notices such as welcome messages and boost events are posted",
        "optional": true,
        "nullable": false
      },
      "system_channel_flags": {
        "type": "integer",
        "description": "system channel flags",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Get Guild": {
    "command": "GET",
    "url": "/guilds/{guild.id}",
    "query": {
      "with_counts": {
        "type": "boolean",
        "description": "when true, will return approximate member and presence counts for the guild",
        "required": "false",
        "default": "false",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Get Guild Preview": {
    "command": "GET",
    "url": "/guilds/{guild.id}/preview"
  },
  "Modify Guild": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}",
    "json": {
      "name": {
        "type": "string",
        "description": "guild name",
        "optional": false,
        "nullable": false
      },
      "region": {
        "type": "string",
        "description": "guild voice region id (deprecated)",
        "optional": false,
        "nullable": true
      },
      "verification_level": {
        "type": "integer",
        "description": "verification level",
        "optional": false,
        "nullable": true
      },
      "default_message_notifications": {
        "type": "integer",
        "description": "default message notification level",
        "optional": false,
        "nullable": true
      },
      "explicit_content_filter": {
        "type": "integer",
        "description": "explicit content filter level",
        "optional": false,
        "nullable": true
      },
      "afk_channel_id": {
        "type": "snowflake",
        "description": "id for afk channel",
        "optional": false,
        "nullable": true
      },
      "afk_timeout": {
        "type": "integer",
        "description": "afk timeout in seconds",
        "optional": false,
        "nullable": false
      },
      "icon": {
        "type": "image data",
        "description": "base64 1024x1024 png/jpeg/gif image for the guild icon (can be animated gif when the server has the ANIMATED_ICON feature)",
        "optional": false,
        "nullable": true
      },
      "owner_id": {
        "type": "snowflake",
        "description": "user id to transfer guild ownership to (must be owner)",
        "optional": false,
        "nullable": false
      },
      "splash": {
        "type": "image data",
        "description": "base64 16:9 png/jpeg image for the guild splash (when the server has the INVITE_SPLASH feature)",
        "optional": false,
        "nullable": true
      },
      "discovery_splash": {
        "type": "image data",
        "description": "base64 16:9 png/jpeg image for the guild discovery splash (when the server has the DISCOVERABLE feature)",
        "optional": false,
        "nullable": true
      },
      "banner": {
        "type": "image data",
        "description": "base64 16:9 png/jpeg image for the guild banner (when the server has the BANNER feature; can be animated gif when the server has the ANIMATED_BANNER feature)",
        "optional": false,
        "nullable": true
      },
      "system_channel_id": {
        "type": "snowflake",
        "description": "the id of the channel where guild notices such as welcome messages and boost events are posted",
        "optional": false,
        "nullable": true
      },
      "system_channel_flags": {
        "type": "integer",
        "description": "system channel flags",
        "optional": false,
        "nullable": false
      },
      "rules_channel_id": {
        "type": "snowflake",
        "description": "the id of the channel where Community guilds display rules and/or guidelines",
        "optional": false,
        "nullable": true
      },
      "public_updates_channel_id": {
        "type": "snowflake",
        "description": "the id of the channel where admins and moderators of Community guilds receive notices from Discord",
        "optional": false,
        "nullable": true
      },
      "preferred_locale": {
        "type": "string",
        "description": "the preferred locale of a Community guild used in server discovery and notices from Discord; defaults to \"en-US\"",
        "optional": false,
        "nullable": true
      },
      "features": {
        "type": "array of guild feature strings",
        "description": "enabled guild features",
        "optional": false,
        "nullable": false
      },
      "description": {
        "type": "string",
        "description": "the description for the guild",
        "optional": false,
        "nullable": true
      },
      "premium_progress_bar_enabled": {
        "type": "boolean",
        "description": "whether the guild's boost progress bar should be enabled",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Delete Guild": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}"
  },
  "Get Guild Channels": {
    "command": "GET",
    "url": "/guilds/{guild.id}/channels"
  },
  "Create Guild Channel": {
    "command": "POST",
    "url": "/guilds/{guild.id}/channels",
    "json": {
      "name": {
        "type": "string",
        "description": "channel name (1-100 characters)",
        "optional": false,
        "nullable": false
      },
      "type": {
        "type": "integer",
        "description": "the type of channel",
        "optional": false,
        "nullable": false
      },
      "topic": {
        "type": "string",
        "description": "channel topic (0-1024 characters)",
        "optional": false,
        "nullable": false
      },
      "bitrate": {
        "type": "integer",
        "description": "the bitrate (in bits) of the voice or stage channel; min 8000",
        "optional": false,
        "nullable": false,
        "note-field": "For voice channels, normal servers can set bitrate up to 96000, servers with Boost level 1 can set up to 128000, servers with Boost level 2 can set up to 256000, and servers with Boost level 3 or the VIP_REGIONS guild feature can set up to 384000. For stage channels, bitrate can be set up to 64000."
      },
      "user_limit": {
        "type": "integer",
        "description": "the user limit of the voice channel",
        "optional": false,
        "nullable": false
      },
      "rate_limit_per_user": {
        "type": "integer",
        "description": "amount of seconds a user has to wait before sending another message (0-21600); bots, as well as users with the permission manage_messages or manage_channel, are unaffected",
        "optional": false,
        "nullable": false
      },
      "position": {
        "type": "integer",
        "description": "sorting position of the channel",
        "optional": false,
        "nullable": false
      },
      "permission_overwrites": {
        "type": "array of partial overwrite objects",
        "description": "the channel's permission overwrites",
        "optional": false,
        "nullable": false,
        "note-field": "In each overwrite object, the allow and deny keys can be omitted or set to null, which both default to \"0\"."
      },
      "parent_id": {
        "type": "snowflake",
        "description": "id of the parent category for a channel",
        "optional": false,
        "nullable": false
      },
      "nsfw": {
        "type": "boolean",
        "description": "whether the channel is nsfw",
        "optional": false,
        "nullable": false
      },
      "rtc_region": {
        "type": "string",
        "description": "channel voice region id of the voice or stage channel, automatic when set to null",
        "optional": false,
        "nullable": false
      },
      "video_quality_mode": {
        "type": "integer",
        "description": "the camera video quality mode of the voice channel",
        "optional": false,
        "nullable": false
      },
      "default_auto_archive_duration": {
        "type": "integer",
        "description": "the default duration that the clients use (not the API) for newly created threads in the channel, in minutes, to automatically archive the thread after recent activity",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Modify Guild Channel Positions": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/channels",
    "json": {
      "id": {
        "type": "snowflake",
        "description": "channel id",
        "optional": false,
        "nullable": false
      },
      "position": {
        "type": "integer",
        "description": "sorting position of the channel",
        "optional": false,
        "nullable": true
      },
      "lock_permissions": {
        "type": "boolean",
        "description": "syncs the permission overwrites with the new parent, if moving to a new category",
        "optional": false,
        "nullable": true
      },
      "parent_id": {
        "type": "snowflake",
        "description": "the new parent ID for the channel that is moved",
        "optional": false,
        "nullable": true
      }
    }
  },
  "List Active Guild Threads": {
    "command": "GET",
    "url": "/guilds/{guild.id}/threads/active"
  },
  "Get Guild Member": {
    "command": "GET",
    "url": "/guilds/{guild.id}/members/{user.id}"
  },
  "List Guild Members": {
    "command": "GET",
    "url": "/guilds/{guild.id}/members",
    "query": {
      "limit": {
        "type": "integer",
        "description": "max number of members to return (1-1000)",
        "default": "1",
        "optional": false,
        "nullable": false
      },
      "after": {
        "type": "snowflake",
        "description": "the highest user id in the previous page",
        "default": "0",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Search Guild Members": {
    "command": "GET",
    "url": "/guilds/{guild.id}/members/search",
    "query": {
      "query": {
        "type": "string",
        "description": "Query string to match username(s) and nickname(s) against.",
        "default": "",
        "optional": false,
        "nullable": false
      },
      "limit": {
        "type": "integer",
        "description": "max number of members to return (1-1000)",
        "default": "1",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Add Guild Member": {
    "command": "PUT",
    "url": "/guilds/{guild.id}/members/{user.id}",
    "json": {
      "access_token": {
        "type": "string",
        "description": "an oauth2 access token granted with the guilds.join to the bot's application for the user you want to add to the guild",
        "permission": "",
        "optional": false,
        "nullable": false
      },
      "nick": {
        "type": "string",
        "description": "value to set user's nickname to",
        "permission": "MANAGE_NICKNAMES",
        "optional": false,
        "nullable": false
      },
      "roles": {
        "type": "array of snowflakes",
        "description": "array of role ids the member is assigned",
        "permission": "MANAGE_ROLES",
        "optional": false,
        "nullable": false
      },
      "mute": {
        "type": "boolean",
        "description": "whether the user is muted in voice channels",
        "permission": "MUTE_MEMBERS",
        "optional": false,
        "nullable": false
      },
      "deaf": {
        "type": "boolean",
        "description": "whether the user is deafened in voice channels",
        "permission": "DEAFEN_MEMBERS",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Modify Guild Member": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/members/{user.id}",
    "json": {
      "nick": {
        "type": "string",
        "description": "value to set user's nickname to",
        "permission": "MANAGE_NICKNAMES",
        "optional": false,
        "nullable": false
      },
      "roles": {
        "type": "array of snowflakes",
        "description": "array of role ids the member is assigned",
        "permission": "MANAGE_ROLES",
        "optional": false,
        "nullable": false
      },
      "mute": {
        "type": "boolean",
        "description": "whether the user is muted in voice channels. Will throw a 400 error if the user is not in a voice channel",
        "permission": "MUTE_MEMBERS",
        "optional": false,
        "nullable": false
      },
      "deaf": {
        "type": "boolean",
        "description": "whether the user is deafened in voice channels. Will throw a 400 error if the user is not in a voice channel",
        "permission": "DEAFEN_MEMBERS",
        "optional": false,
        "nullable": false
      },
      "channel_id": {
        "type": "snowflake",
        "description": "id of channel to move user to (if they are connected to voice)",
        "permission": "MOVE_MEMBERS",
        "optional": false,
        "nullable": false
      },
      "communication_disabled_until": {
        "type": "ISO8601 timestamp",
        "description": "when the user's timeout will expire and the user will be able to communicate in the guild again (up to 28 days in the future), set to null to remove timeout. Will throw a 403 error if the user has the ADMINISTRATOR permission or is the owner of the guild",
        "permission": "MODERATE_MEMBERS",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Modify Current Member": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/members/@me",
    "json": {
      "nick": {
        "type": "string",
        "description": "value to set user's nickname to",
        "permission": "CHANGE_NICKNAME",
        "optional": true,
        "nullable": true
      }
    }
  },
  "Modify Current User Nick": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/members/@me/nick",
    "json": {
      "nick": {
        "type": "string",
        "description": "value to set user's nickname to",
        "permission": "CHANGE_NICKNAME",
        "optional": true,
        "nullable": true
      }
    }
  },
  "Add Guild Member Role": {
    "command": "PUT",
    "url": "/guilds/{guild.id}/members/{user.id}/roles/{role.id}"
  },
  "Remove Guild Member Role": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/members/{user.id}/roles/{role.id}"
  },
  "Remove Guild Member": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/members/{user.id}"
  },
  "Get Guild Bans": {
    "command": "GET",
    "url": "/guilds/{guild.id}/bans",
    "query": {
      "limit": {
        "type": "number",
        "description": "number of users to return (up to maximum 1000)",
        "default": "1000",
        "optional": true,
        "nullable": false
      },
      "before": {
        "type": "snowflake",
        "description": "consider only users before given user id",
        "default": "null",
        "optional": true,
        "nullable": false,
        "note-field": "Provide a user id to before and after for pagination. Users will always be returned in ascending order by user.id. If both before and after are provided, only before is respected."
      },
      "after": {
        "type": "snowflake",
        "description": "consider only users after given user id",
        "default": "null",
        "optional": true,
        "nullable": false,
        "note-field": "Provide a user id to before and after for pagination. Users will always be returned in ascending order by user.id. If both before and after are provided, only before is respected."
      }
    }
  },
  "Get Guild Ban": {
    "command": "GET",
    "url": "/guilds/{guild.id}/bans/{user.id}"
  },
  "Create Guild Ban": {
    "command": "PUT",
    "url": "/guilds/{guild.id}/bans/{user.id}",
    "json": {
      "delete_message_days": {
        "type": "integer",
        "description": "number of days to delete messages for (0-7)",
        "default": "0",
        "optional": true,
        "nullable": false
      },
      "reason": {
        "type": "string",
        "description": "reason for the ban (deprecated)",
        "default": "",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Remove Guild Ban": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/bans/{user.id}"
  },
  "Get Guild Roles": {
    "command": "GET",
    "url": "/guilds/{guild.id}/roles"
  },
  "Create Guild Role": {
    "command": "POST",
    "url": "/guilds/{guild.id}/roles",
    "json": {
      "name": {
        "type": "string",
        "description": "name of the role",
        "default": "\"new role\"",
        "optional": false,
        "nullable": false
      },
      "permissions": {
        "type": "string",
        "description": "bitwise value of the enabled/disabled permissions",
        "default": "@everyone permissions in guild",
        "optional": false,
        "nullable": false
      },
      "color": {
        "type": "integer",
        "description": "RGB color value",
        "default": "0",
        "optional": false,
        "nullable": false
      },
      "hoist": {
        "type": "boolean",
        "description": "whether the role should be displayed separately in the sidebar",
        "default": "false",
        "optional": false,
        "nullable": false
      },
      "icon": {
        "type": "image data",
        "description": "the role's icon image (if the guild has the ROLE_ICONS feature)",
        "default": "null",
        "optional": false,
        "nullable": true
      },
      "unicode_emoji": {
        "type": "string",
        "description": "the role's unicode emoji as a standard emoji (if the guild has the ROLE_ICONS feature)",
        "default": "null",
        "optional": false,
        "nullable": true
      },
      "mentionable": {
        "type": "boolean",
        "description": "whether the role should be mentionable",
        "default": "false",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Modify Guild Role Positions": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/roles",
    "json": {
      "id": {
        "type": "snowflake",
        "description": "role",
        "optional": false,
        "nullable": false
      },
      "position": {
        "type": "integer",
        "description": "sorting position of the role",
        "optional": true,
        "nullable": true
      }
    }
  },
  "Modify Guild Role": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/roles/{role.id}",
    "json": {
      "name": {
        "type": "string",
        "description": "name of the role",
        "optional": false,
        "nullable": false
      },
      "permissions": {
        "type": "string",
        "description": "bitwise value of the enabled/disabled permissions",
        "optional": false,
        "nullable": false
      },
      "color": {
        "type": "integer",
        "description": "RGB color value",
        "optional": false,
        "nullable": false
      },
      "hoist": {
        "type": "boolean",
        "description": "whether the role should be displayed separately in the sidebar",
        "optional": false,
        "nullable": false
      },
      "icon": {
        "type": "image data",
        "description": "the role's icon image (if the guild has the ROLE_ICONS feature)",
        "optional": false,
        "nullable": false
      },
      "unicode_emoji": {
        "type": "string",
        "description": "the role's unicode emoji as a standard emoji (if the guild has the ROLE_ICONS feature)",
        "optional": false,
        "nullable": false
      },
      "mentionable": {
        "type": "boolean",
        "description": "whether the role should be mentionable",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Modify Guild MFA Level": {
    "command": "POST",
    "url": "/guilds/{guild.id}/mfa",
    "json": {
      "level": {
        "type": "integer",
        "description": "MFA level",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Delete Guild Role": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/roles/{role.id}"
  },
  "Get Guild Prune Count": {
    "command": "GET",
    "url": "/guilds/{guild.id}/prune",
    "query": {
      "days": {
        "type": "integer",
        "description": "number of days to count prune for (1-30)",
        "default": "7",
        "optional": false,
        "nullable": false
      },
      "include_roles": {
        "type": "string; comma-delimited array of snowflakes",
        "description": "role(s) to include",
        "default": "none",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Begin Guild Prune": {
    "command": "POST",
    "url": "/guilds/{guild.id}/prune",
    "json": {
      "days": {
        "type": "integer",
        "description": "number of days to prune (1-30)",
        "default": "7",
        "optional": false,
        "nullable": false
      },
      "compute_prune_count": {
        "type": "boolean",
        "description": "whether pruned is returned, discouraged for large guilds",
        "default": "true",
        "optional": false,
        "nullable": false
      },
      "include_roles": {
        "type": "array of snowflakes",
        "description": "role(s) to include",
        "default": "none",
        "optional": false,
        "nullable": false
      },
      "reason": {
        "type": "string",
        "description": "reason for the prune (deprecated)",
        "default": "",
        "optional": true,
        "nullable": false
      }
    }
  },
  "Get Guild Voice Regions": {
    "command": "GET",
    "url": "/guilds/{guild.id}/regions"
  },
  "Get Guild Invites": {
    "command": "GET",
    "url": "/guilds/{guild.id}/invites"
  },
  "Get Guild Integrations": {
    "command": "GET",
    "url": "/guilds/{guild.id}/integrations"
  },
  "Delete Guild Integration": {
    "command": "DELETE",
    "url": "/guilds/{guild.id}/integrations/{integration.id}"
  },
  "Get Guild Widget Settings": {
    "command": "GET",
    "url": "/guilds/{guild.id}/widget"
  },
  "Modify Guild Widget": {
    "command": "PATCH",
    "url": "/guilds/{guild.id}/widget"
  },
  "Get Guild Widget": {
    "command": "GET",
    "url": "/guilds/{guild.id}/widget.json"
  },
  "Get Guild Vanity URL": {
    "command": "GET",
    "url": "/guilds/{guild.id}/vanity-url"
  },
  "Get Guild Widget Image": {
    "command": "GET",
    "url": "/guilds/{guild.id}/widget.png",
    "query": {
      "style": {
        "type": "string",
        "description": "style of the widget image returned (see below)",
        "default": "shield",
        "optional": false,
        "nullable": false
      }
    }
  },
  "Get Guild Welcome Screen": {
    "command": "GET",
    "url": "/guilds/{guild.id}/welcome-screen"
  },
  "Modify Guild Welcome Screen": {
